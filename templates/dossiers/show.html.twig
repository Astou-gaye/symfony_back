{% extends 'base.html.twig' %}

{% block title %}Dossiers{% endblock %}

{% block body %}
    <h1>Dossiers</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dossier.id }}</td>
            </tr>
            <tr>
                <th>Passeport</th>
                <td>{{ dossier.passeport }}</td>
            </tr>
            <tr>
                <th>Visa</th>
                <td>{{ dossier.visa }}</td>
            </tr>
            <tr>
                <th>CarnetSante</th>
                <td>{{ dossier.carnetSante }}</td>
            </tr>
            <tr>
                <th>PhotoIdentite</th>
                <td>{{ dossier.photoIdentite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_dossiers_index') }}">back to list</a>

    <a href="{{ path('app_dossiers_edit', {'id': dossier.id}) }}">edit</a>

    {{ include('dossiers/_delete_form.html.twig') }}
